{
  "name": "pol",
  "displayName": "My Language Support",
  "description": "Support for my language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pol",
      "aliases": ["pol", "pol"],
      "extensions": [".pol"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pol",
      "scopeName": "source.pol",
      "path": "./syntaxes/pol.tmLanguage.json"
    }]
  }
}
